<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.thishouse.mapper.TestMapper">
    <sql id = "testColums">
        idx,
        name
    </sql>

    <insert id="insertTest" parameterType="com.example.thishouse.domain.testDTO">
        insert into test (
        <include refid="testColums"></include>
        ) values(
        #{idx},
        #{name}
        )
    </insert>
</mapper>